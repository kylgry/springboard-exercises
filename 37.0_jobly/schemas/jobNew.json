{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.job.com/jobNew.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1
    },
    "salary": {
      "type": "integer",
      "minimum": 1
    },
    "equity": {
      "type": "integer",
      "minimum": 0,
      "maximum": 1
    },
    "companyHandle": {
      "type": "string",
      "minLength": 1
    }
  },
  "additionalProperties": false,
  "required": [
    "title",
    "companyHandle"
    ]
}
